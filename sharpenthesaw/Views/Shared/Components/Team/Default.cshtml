@{
}

@model IEnumerable<Teams>

<a class="egg btn btn-outline-secondary" asp-action="Index" asp-controller="Home" asp-route-teamid="">Home</a>
<br />

@foreach (var x in Model)
{
    <a class="egg btn @(x.TeamName == ViewBag.SelectedTeam ? "btn-primary" : "btn-outline-secondary") "  asp-action="Index" asp-controller="Home" asp-route-teamid="@x.TeamId" asp-route-teamname="@x.TeamName">@x.TeamName</a>
    <br />
}